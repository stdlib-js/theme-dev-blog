{
  "private": true,
  "name": "journal",
  "version": "1.0.0",
  "description": "A Ghost theme for the official stdlib development blog.",
  "license": "MIT",
  "author": {
    "name": "Ghost Foundation",
    "email": "hello@ghost.org",
    "url": "https://ghost.org"
  },
  "contributors": [
    {
      "name": "Ghost Foundation",
      "url": "https://ghost.org"
    },
    {
      "name": "The Stdlib Authors",
      "url": "https://github.com/stdlib-js/www-dev-blog-theme/graphs/contributors"
    }
  ],
  "scripts": {},
  "main": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/stdlib-js/www-dev-blog-theme.git"
  },
  "homepage": "https://github.com/stdlib-js/www-dev-blog-theme",
  "keywords": [
    "ghost",
    "theme",
    "ghost-theme",
    "stdlib",
    "stdlib-js",
    "stdlib.js",
    "blog"
  ],
  "bugs": {
    "url": "https://github.com/stdlib-js/www-dev-blog-theme/issues"
  },
  "dependencies": {},
  "devDependencies": {},
  "engines": {
    "ghost": ">=4.0.0"
  },
  "config": {
    "posts_per_page": 10,
    "image_sizes": {
        "xs": {
        "width": 150
      },
      "s": {
        "width": 300
      },
      "m": {
        "width": 720
      },
      "l": {
        "width": 960
      },
      "xl": {
        "width": 1200
      },
      "xxl": {
       "width": 2000
      }
    },
    "card_assets": true,
    "custom": {
      "title_font": {
        "type": "select",
        "options": [
          "Modern sans-serif",
          "Elegant serif"
        ],
        "default": "Modern sans-serif"
      },
      "body_font": {
        "type": "select",
        "options": [
          "Modern sans-serif",
          "Elegant serif"
        ],
        "default": "Modern sans-serif"
      }
    }
  }
}
